                                    <TextField fx:id="maskin" editable="false" layoutX="14.0" layoutY="14.0" prefWidth="249.0" text="">
                                       <font>
                                          <Font size="20.0" />
                                       </font>
                                    </TextField>
                                 </children>
                              </AnchorPane>
                           </content>
                        </ScrollPane>
                     </children>
                  </AnchorPane>
                  <AnchorPane id="Content" maxWidth="-Infinity" minHeight="-1.0" minWidth="-Infinity" prefHeight="551.0" prefWidth="166.0">
                     <children>
                        <ImageView fx:id="landing" fitHeight="71.0" fitWidth="71.0" layoutX="85.0" layoutY="6.0" onMouseClicked="#onLandingClick" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="https://i.ibb.co/P4j1Fm4/logo.png" />
                           </image>
                        </ImageView>
                        <ImageView fx:id="profile" fitHeight="71.0" fitWidth="71.0" layoutX="14.0" layoutY="6.0" onMouseClicked="#onProfileClick" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="https://i.ibb.co/41g2LWw/profilepic.png" />
                           </image>
                        </ImageView>
                     </children>
                  </AnchorPane>
               </items>
            </SplitPane>
            <HBox id="HBox" alignment="CENTER_LEFT" spacing="5.0" VBox.vgrow="NEVER">
               <children>
                  <Pane prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
               </children>
               <padding>
                  <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
