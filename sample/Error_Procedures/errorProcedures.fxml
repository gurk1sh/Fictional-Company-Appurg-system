<GridPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Error_Procedures.ErrorProcedureController">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <VBox prefHeight="600.0" prefWidth="900.0">
         <children>
            <MenuBar VBox.vgrow="NEVER">
               <menus>
                  <Menu mnemonicParsing="false" text="Appurg" />
               </menus>
            </MenuBar>
            <SplitPane dividerPositions="0.2505567928730512, 0.5" focusTraversable="true" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
               <items>
                  <AnchorPane maxWidth="220.0" prefHeight="567.0" prefWidth="220.0" style="-fx-background-color: rgb(23,97,160);">
                     <children>
                        <Text fx:id="appurg" fill="WHITE" layoutX="70.0" layoutY="49.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Appurg" wrappingWidth="80.0">
                           <font>
                              <Font size="20.0" />
                           </font>
                        </Text>
                        <Button layoutX="74.0" layoutY="489.0" mnemonicParsing="false" text="Logga ut" />
                        <TextField layoutX="36.0" layoutY="86.0" promptText="Sök" />
                        <Accordion fx:id="accordion" layoutX="23.0" layoutY="139.0" prefHeight="75.0" prefWidth="175.0">
                           <panes>
                              <TitledPane fx:id="titledPaneEP" animated="false" prefHeight="175.0" prefWidth="175.0" text="Felprocedurer">
                                 <content>
                                    <AnchorPane minHeight="-Infinity" minWidth="0.0" prefHeight="175.0" prefWidth="173.0" style="-fx-background-color: null;">
                                       <children>
                                          <ListView fx:id="ErrorProcedures" prefHeight="175.0" prefWidth="175.0" />
                                       </children>
                                    </AnchorPane>
                                 </content>
                              </TitledPane>
                              <TitledPane animated="false" prefHeight="50.0" prefWidth="175.0" text="Arbetsprocedurer">
                                 <content>
                                    <AnchorPane minHeight="-Infinity" minWidth="0.0" prefHeight="175.0" prefWidth="173.0">
                                       <children>
                                          <ListView fx:id="WorkProcedures" onMouseClicked="#goToWorkprocedures" prefHeight="175.0" prefWidth="175.0" />
                                       </children>
                                    </AnchorPane>
                                 </content>
                              </TitledPane>
                           </panes>
                        </Accordion>
                     </children>
                  </AnchorPane>
                  <AnchorPane maxWidth="-Infinity" prefHeight="567.0" prefWidth="500.0">
                     <children>
                        <ScrollPane hbarPolicy="NEVER" prefHeight="567.0" prefWidth="500.0" style="-fx-background-color: rgb(23,97,160);">
                           <content>
                              <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="565.0" prefWidth="500.0" style="-fx-background-color: rgb(23.0, 97.0, 160.0);">
                                 <children>
                                    <Label layoutX="15.0" layoutY="73.0" prefWidth="155.0" text="Felprocedurer" textFill="WHITE">
                                       <font>
                                          <Font size="20.0" />
                                       </font>
                                    </Label>
                                    <ListView fx:id="felprocedurer" layoutX="15.0" layoutY="117.0" prefHeight="420.0" prefWidth="155.0" style="-fx-border-style: solid; -fx-border-width: 2; -fx-border-color: lightblue;" />
                                    <TextFlow fx:id="textFlow" layoutX="208.0" layoutY="117.0" prefHeight="420.0" prefWidth="275.0" style="-fx-background-color: white; -fx-border-style: solid; -fx-border-color: lightblue; -fx-border-width: 2;" />
